{
    "name": "ackee-http-client",
    "version": "2.1.1",
    "description": "",
    "main": "lib/index.js",
    "module": "es/index.js",
    "scripts": {
        "prettier": "prettier --config ./prettier.config.js --write './src/**/*.{js,jsx}'",
        "build:lib": "rm -rf ./lib && cross-env BABEL_ENV=lib babel src --out-dir lib",
        "build:es": "rm -rf ./es && cross-env BABEL_ENV=es babel src --out-dir es",
        "build": "rm -rf ./es ./lib && yarn run build:es & npm run build:lib",
        "prepare": "npm run build",
        "start": "yarn install && yarn run build:es && onchange 'src/**/*.{js,jsx}' -- yarn run build:es"
    },
    "author": "Jiří Čermák <jiri.cermak@ackee.cz>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.ack.ee/Web/http-client"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "babel-eslint": "^9.0.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.9.0",
        "eslint-config-ackee": "^0.9.0",
        "eslint-config-prettier": "^3.3.0",
        "onchange": "^5.1.0",
        "prettier": "^1.15.2",
        "prettier-config-ackee": "^0.0.4"
    },
    "dependencies": {
        "@babel/runtime": "^7.1.5",
        "ackee-redux-token-auth": "2.x.x",
        "axios": "0.18.x",
        "redux-saga": "0.16.x"
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "maintained node versions",
        "not dead",
        "IE > 11"
    ]
}
